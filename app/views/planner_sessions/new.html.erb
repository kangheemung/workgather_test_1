<h1>PlannerSessions#new</h1>
<p>Find me in app/views/planner_sessions/new.html.erb</p>
<div class= "asobinagara">
<h1>ASOBINAGARA</h1>
</div>
<div class="signup">
    <div class="singup_title">
        <span>Sign-in with email</span>
    </div>
    <div class="body">
        <%= form_for :planners, url: planners_login_create_path do |f| %>
            <%= f.label :email%><br>
            <div class="body_box">
            <%= f.text_field :email, :placeholder => "email" %><br>
            </div>
            <%= f.label :password %><br>
            <div class="body_box">
            <%= f.password_field :password, :placeholder => "password" %><br>
            </div>
            <div class="body_box">
                <%= f.submit :Login, class: 'button'%>
            </div>
        <% end %>
    </div>
    <div class= "login">
    <span onclick="location.href = '/planners/new'" >会員登録</span>
    </div>
</div>
